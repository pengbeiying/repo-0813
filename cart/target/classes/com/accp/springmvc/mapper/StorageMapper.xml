<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "mybatis-3-mapper.dtd" "mybatis-3-mapper.dtd" >
<mapper namespace="com.accp.springmvc.mapper.StorageMapper">
	
	<update id="updateQty">
		UPDATE `storage` SET `Qty`=`Qty`-1 WHERE `BookId`=#{BookId};
	</update>
	
	<update id="updateQtyj">
		UPDATE `storage` SET `Qty`=`Qty`+1 WHERE `BookId`=#{BookId};
	</update>

</mapper>